{
  "name": "zenlib",
  "scripts": {
    "build": "lerna exec --ignore \"**/@(storybook|bootstrap)\" -- rollup -c \\$LERNA_ROOT_PATH/rollup.config.js",
    "build:watch": "lerna exec --ignore \"**/@(storybook|bootstrap)\" -- rollup -w -c \\$LERNA_ROOT_PATH/rollup.config.js",
    "bootstrap": "lerna bootstrap",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "eslint": "eslint \"packages/**/*.{js,vue}\"",
    "eslint:fix": "eslint --fix \"packages/**/*.{js,vue}\"",
    "storybook": "cd packages/_storybook_ && npm run storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "bootstrap-vue": "^2.0.0-rc.27",
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lerna": "^3.16.4",
    "prettier": "1.18.2",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "packages/**/*.{js,vue}",
      "!packages/_storybook_/**",
      "!**/node_modules/**",
      "!**/dist/**"
    ]
  }
}
